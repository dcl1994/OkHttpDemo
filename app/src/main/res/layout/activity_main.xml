<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/ok_get_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Get_newThread" />
    <Button
        android:id="@+id/ok_get_string2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Get-NOthread" />

    <Button
        android:id="@+id/ok_post_json"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Post_Json" />
    <Button
        android:id="@+id/ok_post_params"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Post_params" />

    <Button
        android:id="@+id/ok_get_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Get_PICTURE" />

    <Button
        android:id="@+id/ok_post_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="OkHttp_Post_file" />
    <Button
        android:id="@+id/ok_get_http_cache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="http缓存" />
    <Button
        android:id="@+id/ok_get_https_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="访问带有自证书的https" />
    <Button
        android:id="@+id/ok_get_https_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="访问带有自证书的https_双方都需要验证的" />
</LinearLayout>